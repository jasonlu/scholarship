<h1>Listing site_configs</h1>

<table>
  <tr>
    <th>Sitename</th>
    <th>Description</th>
    <th>Keywords</th>
    <th>Copyright</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @site_configs.each do |site_config| %>
  <tr>
    <td><%= site_config.sitename %></td>
    <td><%= site_config.description %></td>
    <td><%= site_config.keywords %></td>
    <td><%= site_config.copyright %></td>
    <td><%= link_to 'Show', site_config %></td>
    <td><%= link_to 'Edit', edit_site_config_path(site_config) %></td>
    <td><%= link_to 'Destroy', site_config, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Site config', new_site_config_path %>
