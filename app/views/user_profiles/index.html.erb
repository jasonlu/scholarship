<h1>Listing user_profiles</h1>

<table>
  <tr>
    <th>User</th>
    <th>Firstname</th>
    <th>Lastname</th>
    <th>Middlename</th>
    <th>Id number</th>
    <th>Dob</th>
    <th>Gender</th>
    <th>Education</th>
    <th>Country</th>
    <th>State</th>
    <th>Register address</th>
    <th>Address</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @user_profiles.each do |user_profile| %>
  <tr>
    <td><%= user_profile.user_id %></td>
    <td><%= user_profile.firstname %></td>
    <td><%= user_profile.lastname %></td>
    <td><%= user_profile.middlename %></td>
    <td><%= user_profile.id_number %></td>
    <td><%= user_profile.dob %></td>
    <td><%= user_profile.gender %></td>
    <td><%= user_profile.education %></td>
    <td><%= user_profile.country %></td>
    <td><%= user_profile.state %></td>
    <td><%= user_profile.register_address %></td>
    <td><%= user_profile.address %></td>
    <td><%= link_to 'Show', user_profile %></td>
    <td><%= link_to 'Edit', edit_user_profile_path(user_profile) %></td>
    <td><%= link_to 'Destroy', user_profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

